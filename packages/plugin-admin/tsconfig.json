{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "ES2016",
    "outDir": "./dist/client",
    "rootDir": "./src/client",
    "strictPropertyInitialization": false,
    "jsx": "react",
    "declaration": false,
    "types": [
      "reflect-metadata"
    ]
  },
  "include": [
    "src/client/**/*.tsx",
    "src/client/**/*.ts"
  ],
  "exclude": [
    "src/plugin/**/*.ts",
  ]
}
